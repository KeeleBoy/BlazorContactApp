@page "/CreateContact"

<form method="post">
    <div>
    <h4>First Name</h4>
    <input type="string" name="firstName">
    </div>
    <div>
    <h4>Last Name</h4>
    <input type="string" name="lastName">
    </div>
    <div>
    <h4>Phone Number</h4>
    <input type="string" name="phoneNumber">
    </div>
    <div>
    <h4>Birthday</h4>
    <input type="datetime-local" name="birthDate">
    </div>
    <input type="submit">
</form>